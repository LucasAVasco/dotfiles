<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Emulator Here</name>
	<unique-id>1616366554260957-1</unique-id>
	<command>exo-open --working-directory &quot;$(test -d %f &amp;&amp; echo %f || dirname %f )&quot; --launch TerminalEmulator</command>
	<description>Open default terminal emulator</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>gtk-directory</icon>
	<name>Open As Root</name>
	<unique-id>1616433888451903-1</unique-id>
	<command>pkexec thunar %f</command>
	<description>Open directory as root</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>system-file-manager</icon>
	<name>Open in new window</name>
	<unique-id>1618778907566447-1</unique-id>
	<command>thunar %F</command>
	<description>Open directory in a new thunar window</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>applications-featured</icon>
	<name>Create Launcher</name>
	<unique-id>1618281542059180-1</unique-id>
	<command>exo-desktop-item-edit --create-new --type Application %f</command>
	<description>Create a new launcher in this directory</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>insert-link</icon>
	<name>Create Web Link launcher</name>
	<unique-id>1618281811273630-2</unique-id>
	<command>exo-desktop-item-edit --create-new --type Link %f</command>
	<description>Create a new web link laucher in this directory</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>emblem-symbolic-link</icon>
	<name>Create SymLink</name>
	<unique-id>1620928505635389-1</unique-id>
	<command>ln -Ts %f %n&quot; (symlink)&quot;</command>
	<description>Only on file or folder at a time</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>link</icon>
	<name>Create Hardlink</name>
	<unique-id>1710624610882749-1</unique-id>
	<command>ln %f %n&quot; (hardlink)&quot;</command>
	<description>Only on file or folder at a time</description>
	<patterns>*</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>preferences-system-search</icon>
	<name>Search files</name>
	<unique-id>1620928820597190-2</unique-id>
	<command>catfish %f</command>
	<description>Serach files with catfish</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>kdiff3</icon>
	<name>Compare files</name>
	<unique-id>1620928887598211-3</unique-id>
	<command>meld %F</command>
	<description>Compare files with Meld</description>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>gtkhash</icon>
	<name>Get Hash</name>
	<unique-id>1624307463424133-1</unique-id>
	<command>gtkhash %F</command>
	<description>Gets the file hash</description>
	<patterns>*</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>data-information</icon>
	<name>Remove metadata (as possible)</name>
	<unique-id>1688445336640407-1</unique-id>
	<command>mat2 %F</command>
	<description>Attempts to remove all metadata (some may remain)</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>data-information</icon>
	<name>Remove some metadata</name>
	<unique-id>1688445681124843-2</unique-id>
	<command>mat2 -L %F</command>
	<description>Attempts to remove all metadata. Less loss in quality (some may remain)</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
