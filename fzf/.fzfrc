# Key bindings
--bind 'alt-d:half-page-down'
--bind 'ctrl-d:half-page-down'
--bind 'alt-u:half-page-up'
--bind 'ctrl-u:half-page-up'
--bind 'tab:toggle' --bind 'tab:+down' # Just move down in non-multi-select mode. Toggle and move down in multi
--bind 'btab:toggle' --bind 'btab:+up' # Just move up in non-multi-select mode. Toggle and move up in multi

# Behavior
--cycle

# Style
--reverse

--color=fg:#d9d9d9,hl:#95c7db
--color=fg+:#ffffff,hl+:#98edf5

--color=prompt:#ffe342

--color=spinner:#ff822e
--color=info:#ffee6b
--color=border:#aaaaaa

--color=header:#9febe6

--color=pointer:#9090f0
--color=marker:#8affc8
--color=gutter:#000000

--color=preview-bg:#222222
--color=preview-fg:#cccccc

